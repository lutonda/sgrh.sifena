<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="sifena\sifenaBundle\Entity\Formacao" table="formacao">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="curso" type="string" column="curso" length="50" nullable="false"/>
    <field name="descricao" type="string" column="descricao" length="200" nullable="true"/>
    <field name="grauAcademico" type="string" column="grau_academico" length="2" nullable="true"/>
  </entity>
</doctrine-mapping>
