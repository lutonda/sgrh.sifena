{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Contrato</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Datainicio</th>
                <td>{{ entity.dataInicio|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Datafim</th>
                <td>{{ entity.dataFim|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Estado</th>
                <td>{{ entity.estado }}</td>
            </tr>
            <tr>
                <th>Observacao</th>
                <td>{{ entity.observacao }}</td>
            </tr>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('contrato') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('contrato_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
